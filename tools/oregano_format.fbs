// Eventual output format for linters
namespace Oregano;

table Check {
    name:string;
    tool:string;
    message:string;
    severity:string;
    line:uint16;
    column:uint16;
}

table File {
    file_name:string;
    checks:[Check];
}

table Run {
    files:[File];
    exit_code:uint16;
    file_prefix:string;
}

root_type:[Run];

